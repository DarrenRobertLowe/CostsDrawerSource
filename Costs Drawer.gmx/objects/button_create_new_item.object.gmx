        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (shown)
{
    /// DRAW self, mouseOver event;
    x = (view_xview[0] + startX);
    y = (view_yview[0] + startY);
    
    var scale = global.GUI_SCALE;
    //image_xscale = scale;
    //image_yscale = scale;
    
    scaleX = 0;
    if ( scale == 1.5 ) then scaleX = 60;//(scale * GUI_SCALE_X_MULTIPLIER);
    
    
      
    x = view_xview[0] + startX + scaleX;
    
    draw_self_round();
    
    
    
    
    if  (global.MOUSE_FREE = true)
    and (global.menuSelected == 0)
    {
        if (mouseOverArea(startX+scaleX, startY, startX+scaleX+30, startY+30))
        then tooltip("Create New Blank Item");
    }
    
    if  (global.MOUSE_FREE = true)
    and (global.menuSelected == 0)
    and (view_yview[0] &lt; GENERAL_INSTRUCTIONS_FEE.y)
    {
        if ( scale == 1.5 )
          then sprite_index = spr_create_new_item_150;
          else sprite_index = spr_create_new_item;
        
        // CREATE AT SELECTED POSITION
        if  exists(global.selected)
        and ( global.selected.object_index != item_caseDetails )
        and ( find_list(global.selected) == global.listOfItems )
        {
            if (mouseOverArea(startX+scaleX, startY, startX+scaleX+30, startY+30))
            {
                highlightButton();
                
                // MOUSE CLICK
                if ( mouse_check_button_pressed(mb_left) )
                {
                    var pos = ds_list_find_index(global.listOfItems, global.selected)
                    insert_item(USER_ITEM, pos+1);
                }
            }
        }
        
        // ELSE CREATE AT END OF LIST (before BUTTON_CreateNewItem)
        else
        {
            if (mouseOverArea(startX+scaleX, startY, startX+scaleX+30, startY+30))
            {
                highlightButton();
                
                // MOUSE CLICK
                if (mouse_check_button_pressed(mb_left))
                {
                    var pos = ds_list_find_index(global.listOfItems, BUTTON_CreateNewItem.id)
                    
                    if (pos &gt; -1) then insert_item(USER_ITEM, pos);
                }
            }
        }
    }
    else
    {
        if ( scale == 1.5 )
          then sprite_index = spr_create_new_item_gray_150;
          else sprite_index = spr_create_new_item_gray;
    }
}
</string>
