        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// DRAW self, mouseOver event;

if (shown)
{
    x = (view_xview[0] + startX);
    y = (view_yview[0] + startY);
    
    var scale = global.GUI_SCALE;
    scaleX = 0;
    if ( scale == 1.5 ) then scaleX = 70;
    
    
    x = (view_xview[0] + startX + scaleX);
    draw_self_round();
    
    
    
    if  (global.MOUSE_FREE = true)
    and (global.menuSelected == 0)
    {
    
        if (mouseOverArea(startX+scaleX,startY,startX+scaleX+30,startY+30))
        {
            tooltip("Delete Selected Item");
        }
        
        if  (global.selected &gt; -1)
        and exists(global.selected)
        and (global.selected.canDelete == true)
        {
            if (scale == 1.5)
              then sprite_index = spr_button_delete_item_150;
              else sprite_index = spr_button_delete_item;
              
            if (mouseOverArea(startX+scaleX, startY, startX+scaleX+30, startY+30))
            {
                highlightButton();
                
                // MOUSE CLICK
                if (mouse_check_button_pressed(mb_left))
                {
                    if (show_question(DELETE_ITEM_MESSAGE))
                    {
                        delete_item(global.selected)
                    }
                }
            }
        }
        else
        {
            if (scale == 1.5)
              then sprite_index = spr_button_delete_item_gray_150;
              else sprite_index = spr_button_delete_item_gray;
        }
    }
    else
    {
        if (scale == 1.5)
          then sprite_index = spr_button_delete_item_gray_150;
          else sprite_index = spr_button_delete_item_gray;
    }
}
</string>
