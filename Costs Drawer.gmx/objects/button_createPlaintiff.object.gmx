            delbtn.myValue  = name;
            
            var edtbtn = instance_create(x,y,button_editPlaintiff);
            edtbtn.myNumber = ds_list_size(global.PLAINTIFFS);
            edtbtn.myValue  = name;
            
            plaintiffs_SCROLLBAR_H.alarm[0] = 1;
