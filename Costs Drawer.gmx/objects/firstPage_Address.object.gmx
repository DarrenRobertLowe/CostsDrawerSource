          else draw_set_color(global.itemTextColor);
          
        if  (addressLine1 == defaultAddressLine1)
        {
            if (global.PRINT_VIEW == false) then draw_text(textPosX,textPosY,addressLine1);
        }
        else draw_text(textPosX,textPosY,addressLine1);
