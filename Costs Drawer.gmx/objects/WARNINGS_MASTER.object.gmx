        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// create the list of rows with cautions and warnings

ds_list_clear(warningRowList);
ds_list_clear(cautionRowList);


// create a list of rows with warnings on them
list = (global.WARNINGS);

if (ds_list_size(list) &gt; 0)
{
    for(var i=0; i&lt;ds_list_size(list); i++)
    {
        var errStr = ds_list_find_value(list,i);
        
        // find the colon :
        var colPos = string_pos(":",errStr);
        
        // grab the row from the error string
        var row = real(string_copy(errStr,5,colPos));
        
        // add the row if it's new
        if (ds_list_find_index(warningRowList,row) == -1) then ds_list_add(warningRowList,row);
    }
}


// create a list of rows with cautions on them
list = (global.CAUTIONS);

if (ds_list_size(list) &gt; 0)
{
    for(var i=0; i&lt;ds_list_size(list); i++)
    {
        var errStr = ds_list_find_value(list,i);
        
        // find the colon :
        var colPos = string_pos(":",errStr);
        
        // grab the row from the error string
        var row = real(string_copy(errStr,5,colPos));
        
        // add the row if it's new
        if (ds_list_find_index(cautionRowList,row) == -1) then ds_list_add(cautionRowList,row);
    }
}
