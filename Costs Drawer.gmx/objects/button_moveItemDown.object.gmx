        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (shown)
{
    /// DRAW self, mouseOver event;
    x = (view_xview[0] + startX);
    y = (view_yview[0] + startY);
    
    var scale = global.GUI_SCALE;
    //image_xscale = scale;
    //image_yscale = scale;
    
    scaleX = 0;
    if ( scale == 1.5 ) then scaleX = 100;
    
    if ( scale == 1.5 )
      then sprite_index = spr_moveItemDown_150;
      else sprite_index = spr_moveItemDown;
    
    
    x = (view_xview[0] + startX + scaleX);
    
    
    
    var xv = x-view_xview[0];
    var yv = y-view_yview[0];
    if (mouseOverArea(xv,yv,xv+30,yv+30))
    {
        tooltip("Move selected item down");
    }
    
    
    if  (global.MOUSE_FREE = true)
    and (global.menuSelected == 0)
    and (global.selected &gt; -1)
    {
        if  !(is_undefined(global.selected))
        and ( instance_exists(global.selected) )
        and (global.selected.moveable == true)
        {
            list = find_list(global.selected);
            
            if (list &gt; -1)
            {
    
                if (mouseOverArea(xv,yv,xv+30,yv+30))
                {   
                    highlightButton();
                    
                    // MOUSE CLICK
                    if ( mouse_check_button_pressed(mb_left) )
                    {
                        move_item_down_list(list, global.selected);
                    }
                }
            }
        }
        else
        {
            if (scale == 1.5)
              then sprite_index = spr_moveItemDown_gray_150;
              else sprite_index = spr_moveItemDown_gray;
        }
    }
    else
    {
        if (scale == 1.5)
          then sprite_index = spr_moveItemDown_gray_150;
          else sprite_index = spr_moveItemDown_gray;
    }
    
    
    draw_self_round();
}
</string>
