            <string>/// mouse click events

if (SCROLLBAR_H.scrollbarShown = true)
{
    /* count down */

    if delayBeforeButtonRepeat &gt; -1 {                               // -1 is lowest
         delayBeforeButtonRepeat --;                                // count down while button is held down
    }
    
    
    /* Functionality */
    if mouseOverSprite(){                                              // if the mouse is over this button
        if mouse_check_button(mb_left) {                            // if the user is pressing this button
            if (delayBeforeButtonRepeat &lt;= -1) {
                with SCROLLBAR_H 
                    {
                    x -= global.horizontalScrollAmount;
                    constrain_scrollbarH();
                    get_view_x_from_scrollbar();
                    }
                    
                delayBeforeButtonRepeat =
                        (global.buttonTimerDelay * room_speed);
            }
    
            /* start repeating button automatically */
            if (delayBeforeButtonRepeat == 0) {
                with SCROLLBAR_H 
                    {
                    x -= global.horizontalScrollAmount;
                    constrain_scrollbarH();
                    get_view_x_from_scrollbar();
                    }
                    
                delayBeforeButtonRepeat = (global.hScrollbarButtonRepeatSpeed * room_speed);
            }
        }
    }
    else{
        delayBeforeButtonRepeat = -1;
    }
    
    
    /* Change the SPRITE */
    if mouseOverSprite() {
        if mouse_check_button(mb_left) {
            image_index = 2;
        }
        else image_index = 1;
    }
    else image_index = 0;
    
}
</string>
