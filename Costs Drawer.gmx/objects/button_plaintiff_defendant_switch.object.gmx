        {
            highlightButton();
            
            if (global.BILL_ON_BEHALF_OF == "Plaintiff")
              then tooltip("Switch to Defendant");
              else tooltip("Switch to Plaintiff");
            
            // MOUSE CLICK
            if (mouse_check_button_pressed(mb_left))
            {
                if (global.BILL_ON_BEHALF_OF == "Plaintiff")
                  then global.BILL_ON_BEHALF_OF = "Defendant";
                  else global.BILL_ON_BEHALF_OF = "Plaintiff";
                  
                if exists(firstPage_CostsType)
                {
                    with(firstPage_CostsType)
                    {
                        event_user(0);
                    }
                }
            }
        }
    }
}
