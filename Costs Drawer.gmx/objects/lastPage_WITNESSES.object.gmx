            <string>/// DRAW TEXT

lines = 1; // Only 1 because WITNESSes are moved down a row themselves.

if ( in_view() )
{
    reset_text();
    
    textPosX = global.descriptionColumnX;
    textPosY = getYFromRow(row+1);
    
    draw_text(textPosX,textPosY,"WITNESSES EXPENSES");
    
    var underlineX2  = (textPosX + string_width("WITNESSES EXPENSES"));
    draw_line(textPosX,textPosY,underlineX2,textPosY);
    
    /// NOTE!
    // The following was removed due to an error that would lead to the TOTALS being 0.00.
    // For some reason, when Witnesses were on the next page, a PAGE_NUMBER wouldn't be
    // created for that page, and thus no totals were carried over.
    
    if  (global.PRINT_VIEW == false)
    and (MOVE_BUTTON_ENABLED == true)
    {
        buttonX = (global.outlayColumnX - 32);
        buttonY = (textPosY - 24);
        
        if (global.startWitnessesOnNewPage == false) then draw_sprite(spr_startOnNextPage_on,0,buttonX,buttonY);
        if (global.startWitnessesOnNewPage == true)  then draw_sprite(spr_startOnNextPage_off,0,buttonX,buttonY);
        
        if ( mouseOverMoveToNextPageButton(buttonX, buttonY, buttonX+32, buttonY+32) )
        {
            // highlight the button
            var x1 = (buttonX);
            var y1 = (buttonY);
            var x2 = (buttonX+24+1);
            var y2 = (buttonY+24+1);
            highlight_area(x1,y1,x2,y2);
            
            if (global.startWitnessesOnNewPage == false) then tooltip("Move Witnesses to start of next page");
            if (global.startWitnessesOnNewPage == true)  then tooltip("Move Witnesses to previous page");
            
            if ( mouse_check_button_released(mb_left) )
            {
                global.startWitnessesOnNewPage = !(global.startWitnessesOnNewPage)
                //DOC_ORGANIZER.startFrom = GENERAL_INSTRUCTIONS_FEE.id;
                //DOC_ORGANIZER.alarm[0] = 1;
                
                update_entire_document();
                
                set_unsaved();
            }
        }
    }
    
}
</string>
