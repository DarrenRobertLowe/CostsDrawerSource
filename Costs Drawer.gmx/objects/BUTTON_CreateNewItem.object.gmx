        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// LEFT CLICKING

if (global.PRINT_VIEW == false)
{
    // DRAW SELF
    x1 = global.dateColumnX;
    x2 = global.hColumnX;
    y1 = borderStartY;
    y2 = borderEndY;
    var selected = global.selected;
    var mouseIsOver = false;
    
    
    // setup the color
    draw_set_alpha(1);
    draw_set_font_scaled(font_Times);
    draw_set_halign(fa_center);
    draw_set_color(global.highlightColor);
    draw_set_alpha(0.5);
    
    // check if we can click on the item
    if ( mouseOverCreateNewItem() )
    {
        if (selected &gt; -1)
        {
            if (selected.selectedField == "none")
            or (selected.selectedField == "")
              then mouseIsOver = true;
              //else mouseIsOver = false;
        }
        else mouseIsOver = true;
    }
    //else mouseIsOver = true;
    
    
    
    // draw background rectangle
    if !( mouseOverAnyGUI() )      
    and ( mouseIsOver == true )
    {
        // draw rectangle backdrop...
        drawBorder("create new item");
        draw_set_color(global.highlightTextColor);  // setup color for text
    }
    else // ( mouseIsOver == false )
    {
        draw_set_color(global.highlightTextColor);
        draw_rectangle(x1,y1,x2,y2,false);
        draw_set_color(global.highlightColor);      // setup color for text
    }
    
    
    // draw the text
    draw_set_alpha(1);
    var xx = (global.descriptionColumnX + (global.descriptionColumnWidth/2));
    draw_text(                                              // draw_text(...
        xx,                                                 // x,
        getYFromRow(row+1),                                 // y,   (row+1 because of fa_bottom I think, but changing it doesn't fix it)
        description                                         // string
        );
    
    
        
    // INTERACT
    if  ( in_view() )
    and ( global.PRINT_VIEW == false )
    {
        if (selectedField == "description") // enter has been pressed via keyboard_CURSOR
        {
            alarm[0] = 1;
            selectedField = "none";
        }
        else // clicking with mouse
        {
            if !( mouseOverAnyGUI() )      
            and ( mouseIsOver == true )
            {
                // LEFT-MOUSE CLICK
                if mouse_check_button_pressed(mb_left)
                or keyboard_check_released(vk_enter)
                {
                    alarm[0]=1;
                }
            }
        }
    }
    

    reset_text();   // reset text drawing variables to default


}
</string>
